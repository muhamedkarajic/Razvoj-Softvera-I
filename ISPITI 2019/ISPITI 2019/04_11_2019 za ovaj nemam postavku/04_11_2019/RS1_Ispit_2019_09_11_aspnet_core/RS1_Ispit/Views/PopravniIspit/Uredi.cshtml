@model  RS1_Ispit_asp.net_core.ViewModels.PopravniIspitUrediVM
@{
    ViewData["Title"] = "Uredi";
}

<h2>Uredi</h2>

<form >
    <div class="form-group">
        <label>Predmet: @Model.Predmet</label>
    </div>
    <div class="form-group">
        <label>Razred: @Model.Razred</label>
    </div>
    <div class="form-group">
        <label>Datum:@Model.Datum </label>
        
    </div>
    <div class="form-group">
        <label>Skola: @Model.Skola</label>
      
    </div>

    <div class="form-group">
        <label>Skolska godina: @Model.SkolskaGodina</label>
    </div>
</form>

<div id="ajaxDiv">

</div>

<script>
    $.get("/AjaxStavke/Index/@Model.PopravniIspitId", function (data, status) {
        $("#ajaxDiv").html(data);
    });
</script>